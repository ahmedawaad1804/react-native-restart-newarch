{
  "name": "react-native-restart-newarch",
  "version": "1.0.41",
  "description": "A React Native module for restarting the app programmatically, supporting both the new and old architectures.",
  "license": "MIT",
  "source": "src/index.tsx",
  "main": "./lib/commonjs/index.js",
  "react-native": "src/index.tsx",
  "module": "./lib/module/index.js",
  "types": "lib/typescript/commonjs/src/index.d.ts",
  "scripts": {
    "typecheck": "tsc --noEmit --project tsconfig.test.json",
    "test": "jest",
    "build": "bob build",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  },
  "keywords": [
    "react-native",
    "react-native-restart",
    "react-native-restart-newarch",
    "react-native-restart-new-architecture",
    "ios",
    "android"
  ],
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "workspaces": [
    "example"
  ],
  "author": "ahmedawaad1804 <ahmedawaad1804@gmail.com> (https://github.com/ahmedawaad1804)",
  "peerDependencies": {
    "react-native": ">=0.60.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmedawaad1804/react-native-restart-newarch.git"
  },
  "homepage": "https://github.com/ahmedawaad1804/react-native-restart-newarch#readme",
  "readme": "https://github.com/ahmedawaad1804/react-native-restart-newarch#readme",
  "bugs": {
    "url": "https://github.com/ahmedawaad1804/react-native-restart-newarch/issues"
  },
  "codegenConfig": {
    "name": "RNRestartSpec",
    "type": "modules",
    "jsSrcsDir": "src/codegenSpec",
    "outputDir": {
      "ios": "ios/generated",
      "android": "android/generated"
    },
    "android": {
      "javaPackageName": "com.restart"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@babel/runtime": "^7.21.0",
    "@eslint/js": "^9.8.0",
    "@react-native/eslint-config": "^0.74.86",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "@types/react": "^18.0.28",
    "@types/react-native": "^0.71.3",
    "babel-jest": "^29.7.0",
    "eslint": "9.x",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.5",
    "react-native": "^0.74.4",
    "react-native-builder-bob": "^0.29.0",
    "ts-jest": "^29.2.4",
    "tslib": "^2.6.3",
    "turbo": "^2.0.11",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0"
  },
  "packageManager": "yarn@3.6.4",
  "resolutions": {
    "@types/react": "^18.2.44"
  },
